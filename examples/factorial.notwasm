function factorial(n : i32) : i32 {
    var result = 1;
    while (n > 0) {
        result = result * n;
        n = n - 1;
    }
    return result;
}

function main() : i32 {
    var total = 0;
    var counter = 10000000;    
    while (counter > 0) {
        var n = 10;
        var tmp = factorial(n);
        total = total + tmp;
        counter = counter - 1;
    }
    return total;
}