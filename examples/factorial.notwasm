function factorial(n : i32) : i32 {
    var result : i32 = 1;
    while (n > 0) {
        result = result * n;
        n = n - 1;
    }
    return result;
}

function main() : i32 {
    var total : i32 = 0;
    var counter : i32 = 1;    
    while (counter > 0) {
        var n : i32 = 10;
        total = factorial(n);
        counter = counter - 1;
    }
    return total;
}